<p id="notice"><%= notice %></p>

<h1>Listing Dev Methods</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dev_methods.each do |dev_method| %>
      <tr>
        <td><%= dev_method.name %></td>
        <td><%= link_to 'Show', dev_method %></td>
        <td><%= link_to 'Edit', edit_dev_method_path(dev_method) %></td>
        <td><%= link_to 'Destroy', dev_method, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dev method', new_dev_method_path %>
